{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "declaration": false,
    "emitDecoratorMetadata": false,
    "experimentalDecorators": true,
    "lib": [
      "es5",
      "es2015",
      "es2016",
      "es2017",
      "dom",
      "esnext.asynciterable"
    ],
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "removeComments": true,
    "sourceMap": true,
    "jsx": "react",
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es2017",
    "baseUrl": "./src",
    "paths": {
      "containers/*": ["containers/*"],
      "helpers/*": ["helpers/*"],
      "redux-store/*": ["redux-store/*"],
      "routes/*": ["routes/*"],
      "graqhql-types": ["graqhql-types"],
      "types/*": ["types/*"]
    }
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "node_modules"
  ],
  "compileOnSave": false,
  "atom": {
    "rewriteTsconfig": false
  }
}
